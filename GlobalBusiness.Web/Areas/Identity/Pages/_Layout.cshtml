<!DOCTYPE html>
<html lang="en">
<head>
    <title>@ViewBag.Title - Global Business</title>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0, user-scalable=0" name="viewport" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />

    <!-- Material Dashboard CSS -->
    <link href="~/management/css/loader.css" rel="stylesheet" />


    <!-- Black Dashboard CSS -->
    <link href="~/management/css/dashboard/black-dashboard.min.css" rel="stylesheet" />
    @RenderSection("Styles", false)
    <link href="~/management/css/style.css" rel="stylesheet" />

    <link href="~/management/app.css" rel="stylesheet" />
    <link href="~/management/css/plugins/jquery-confirm.min.css" rel="stylesheet" />

    <!-- Plugins CSS -->
    <link href="~/management/plugins/toastr.js/toastr.min.css" rel="stylesheet" />
    <link rel="shortcut icon" href="/management/img/fav.png" />
</head>
<body class="dashboard">
    <partial name="/Areas/Management/Views/Shared/_PreLoader.cshtml" />

    @RenderBody()
    <partial name="/Areas/Management/Views/Shared/_Footer.cshtml" />

    @RenderSection("Modals", false)
    <!--   Core JS Files   -->
    <script src="~/management/js/core/jquery.min.js"></script>
    <script src="~/management/js/core/popper.min.js"></script>
    <script src="~/management/js/core/bootstrap.min.js"></script>
    <!-- Plugin for the Perfect Scrollbar -->
    <script src="~/management/js/plugins/perfect-scrollbar.jquery.min.js"></script>
    <script src="~/management/js/plugins/jquery.validate.min.js"></script>
    <script src="~/management/js/plugins/jquery.datatables.min.js"></script>
    <script src="~/management/js/plugins/bootstrap-notify.js"></script>

    <script src="~/management/js/plugins/blockui.min.js"></script>
    <!-- Control Center for Material Dashboard: parallax effects, scripts for the example pages etc -->
    <script src="~/management/js/dashboard/black-dashboard.min.js" type="text/javascript"></script>

    <!-- Plugins JS -->
    <script src="~/management/plugins/toastr.js/toastr.min.js"></script>
    @RenderSection("Components", false)

    <script src="~/management/app.js"></script>

    <partial name="/Areas/Management/Views/Shared/_ValidationScriptsPartial.cshtml" />
    @RenderSection("Scripts", required: false)
</body>
</html>
